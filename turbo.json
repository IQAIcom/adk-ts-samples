{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"],
			"cache": true
		},
		"dev": {
			"persistent": true,
			"cache": false,
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		},
		"lint": {
			"cache": false
		},
		"format": {
			"cache": false
		}
	}
}
