{
	"name": "crypto-tax-agent",
	"version": "1.0.0",
	"description": "AI-powered crypto tax assistant that fetches on-chain transactions, calculates cost basis using FIFO/LIFO/HIFO methods, and generates tax reports",
	"main": "dist/index.js",
	"private": true,
	"scripts": {
		"build": "tsc",
		"dev": "tsx watch src/index.ts",
		"start": "node dist/index.js",
		"clean": "rimraf dist",
		"prebuild": "pnpm clean"
	},
	"dependencies": {
		"@iqai/adk": "^0.5.6",
		"dotenv": "^17.2.2",
		"viem": "^2.21.60",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@types/node": "^24.3.1",
		"rimraf": "^6.0.1",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	}
}
