{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"module": "NodeNext",
		"outDir": "./dist",
		"rootDir": "./src",
		"moduleResolution": "nodenext",
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"],
			"@/env": ["./src/env"]
		}
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "dist"]
}
