# Telegram Personal Assistant Configuration
# Copy this file to .env and fill in your values

# Enable debug logging (true/false)
ADK_DEBUG=false

# Required: Google AI API key for Gemini model access
GOOGLE_API_KEY=

# Required: PostgreSQL database connection string
DATABASE_URL=

# Required: Telegram bot token from @BotFather
TELEGRAM_BOT_TOKEN=

# Required: Telegram channel/chat ID for notifications
TELEGRAM_CHANNEL_ID=
